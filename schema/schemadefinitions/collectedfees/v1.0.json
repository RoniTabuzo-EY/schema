{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "feeCollected",
    "timestamp",
    "feePaymentStatus"
  ],
  "properties": {
    "feeCollected": {
      "$id": "#/properties/feeCollected",
      "type": "string",
      "title": "The Feecollected Schema",
      "default": "",
      "examples": [
        "500"
      ],
      "pattern": "^(.*)$"
    },
    "timestamp": {
      "$id": "#/properties/timestamp",
      "type": "string",
      "title": "The Timestamp Schema",
      "default": "",
      "examples": [
        "2019-12-02 15:58:000"
      ],
      "pattern": "^(.*)$"
    },
    "feePaymentStatus": {
      "$id": "#/properties/feePaymentStatus",
      "type": "string",
      "title": "The Feepaymentstatus Schema",
      "default": "",
      "examples": [
        "Pending Payment"
      ],
      "pattern": "^(.*)$"
    }
  }
}